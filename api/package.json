{
  "name": "workowl-api",
  "version": "0.1.0",
  "description": "Api server for workowl service.",
  "main": "index.js",
  "scripts": {
    "start": "node ./bin/www",
    "dev": "concurrently \"npm run dev:api\" \"npm run dev:client\"",
    "dev:api": "nodemon ./bin/www",
    "dev:client": "node ./bin/start-client.js",
    "deploy:db": "node ./scripts/buildDB.js",
    "deploy:manager": "plop --plopfile ./scripts/generateManager.js"
  },
  "keywords": [
    "workowl"
  ],
  "author": "uandi4446@gmail.com",
  "license": "ISC",
  "devDependencies": {
    "eslint": "^5.6.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0"
  },
  "dependencies": {
    "bluebird": "^3.5.2",
    "debug": "^4.0.1",
    "dotenv": "^6.0.0",
    "koa": "^2.5.3",
    "koa-router": "^7.4.0",
    "koa-static": "^5.0.0",
    "mysql2": "^1.6.1",
    "plop": "^2.1.0",
    "sequelize": "^4.39.0"
  }
}
